{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "components": "./src/components",
          "constants": "./src/constants",
          "queries": "./src/queries",
          "style": "./src/style"
        }
      }
    ]
  ]
}
